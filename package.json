{
  "name": "mecha-client",
  "version": "0.0.1",
  "description": "Client application for Mecha-Type",
  "author": "Faustino Zanetto",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start:windows": "next dev --port 3000",
    "start:tunnel": "ngrok http 3000",
    "start:prod": "cross-env NODE_ENV=production next start",
    "build": "yarn test:once:group:no-integration && next build",
    "build:fast": "next build",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:once": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:once:group:no-integration": "cross-env NODE_ENV=test jest --group=-integration",
    "lint": "esw src/ -w --ext .ts --ext .tsx",
    "lint:once": "eslint src/ --ext .ts --ext .tsx",
    "lint:fix": "eslint src/ --ext .ts --ext .tsx --fix",
    "lint:fix:preview": "eslint src/ --ext .ts --ext .tsx --fix-dry-run",
    "format": "npm run lint -- --fix",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "security:audit": "yarn audit",
    "graphql-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@ctrl/react-adsense": "^1.3.1",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@meronex/icons": "^4.0.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@sentry/browser": "^6.13.2",
    "@sentry/node": "^6.13.2",
    "@unly/universal-language-detector": "^2.0.3",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "chart.js": "^3.5.1",
    "cookies": "^0.8.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "graphql": "^15.6.0",
    "howler": "^2.2.3",
    "http-proxy-middleware": "^2.0.1",
    "js-cookie": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.noop": "^3.0.1",
    "next": "^11.1.2",
    "next-apollo": "^5.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-cookies": "^2.0.3",
    "next-google-ads": "^0.1.1",
    "next-i18next": "^8.8.0",
    "next-redux-wrapper": "^7.0.5",
    "next-routes": "^1.4.2",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9",
    "zustand": "^3.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/chalk": "^2.2.0",
    "@types/cookies": "^0.7.7",
    "@types/dotenv": "^8.2.0",
    "@types/howler": "^2.2.4",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash.noop": "^3.0.6",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@zeit/next-source-maps": "^0.0.3",
    "babel-eslint": "10.x",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-watch": "7.0.0",
    "jest": "^27.2.1",
    "jest-cli": "^27.2.1",
    "jest-expect-message": "1.0.2",
    "jest-extended": "^0.11.5",
    "ngrok": "^4.2.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
