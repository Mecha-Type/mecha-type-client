{
  "name": "mecha-client",
  "version": "0.3.5",
  "description": "Client application for Mecha-Type",
  "author": "Faustino Zanetto",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start:dev": "ts-node --project tsconfig.server.json -r tsconfig-paths/register server/index.ts",
    "start:tunnel": "ngrok http 3000",
    "start:prod": "cross-env NODE_ENV=production next start",
    "build": "yarn next build",
    "build:fast": "next build",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:once": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:once:group:no-integration": "cross-env NODE_ENV=test jest --group=-integration",
    "lint": "esw src/ -w --ext .ts --ext .tsx",
    "lint:once": "eslint src/ --ext .ts --ext .tsx",
    "lint:fix": "eslint src/ --ext .ts --ext .tsx --fix",
    "lint:fix:preview": "eslint src/ --ext .ts --ext .tsx --fix-dry-run",
    "format": "npm run lint -- --fix",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "security:audit": "yarn audit",
    "graphql-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@chakra-ui/icons": "1.1.7",
    "@chakra-ui/react": "1.8.6",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@fontsource/poppins": "^4.5.2",
    "@meronex/icons": "4.0.0",
    "@next/bundle-analyzer": "12.1.0",
    "apexcharts": "3.33.2",
    "axios": "0.26.0",
    "chalk": "chalk",
    "cookies": "0.8.0",
    "cross-env": "7.0.3",
    "deepmerge": "4.2.2",
    "dotenv": "16.0.0",
    "formik": "2.2.9",
    "framer-motion": "6.2.8",
    "graphql": "16.3.0",
    "howler": "2.2.3",
    "js-cookie": "3.0.1",
    "lodash.isequal": "4.5.0",
    "lodash.noop": "3.0.1",
    "next": "12.1.0",
    "next-apollo": "5.0.8",
    "next-compose-plugins": "2.2.1",
    "next-google-ads": "^0.1.1",
    "next-i18next": "10.5.0",
    "next-seo": "5.1.0",
    "react": "17.0.2",
    "react-apexcharts": "1.3.9",
    "react-colorful": "^5.5.1",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-ssr-adsense": "^1.0.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "ts-node": "^10.6.0",
    "uuid": "8.3.2",
    "yup": "0.32.11",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@next/eslint-plugin-next": "^12.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/chalk": "^2.2.0",
    "@types/cookies": "^0.7.7",
    "@types/dotenv": "^8.2.0",
    "@types/howler": "^2.2.5",
    "@types/jest": "^27.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash.noop": "^3.0.6",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@zeit/next-source-maps": "^0.0.3",
    "babel-eslint": "10.x",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-watch": "8.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-expect-message": "1.0.2",
    "jest-extended": "^2.0.0",
    "ngrok": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  }
}
