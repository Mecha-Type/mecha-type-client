{
  "name": "mecha-client",
  "version": "0.0.10",
  "description": "Client application for Mecha-Type",
  "author": "Faustino Zanetto",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start:windows": "ts-node --project tsconfig.server.json -r tsconfig-paths/register server/index.ts",
    "start:tunnel": "ngrok http 3000",
    "start:prod": "cross-env NODE_ENV=production next start",
    "build": "yarn test:once:group:no-integration && next build",
    "build:fast": "next build",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:once": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:once:group:no-integration": "cross-env NODE_ENV=test jest --group=-integration",
    "lint": "esw src/ -w --ext .ts --ext .tsx",
    "lint:once": "eslint src/ --ext .ts --ext .tsx",
    "lint:fix": "eslint src/ --ext .ts --ext .tsx --fix",
    "lint:fix:preview": "eslint src/ --ext .ts --ext .tsx --fix-dry-run",
    "format": "npm run lint -- --fix",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "security:audit": "yarn audit",
    "graphql-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@chakra-ui/icons": "1.0.16",
    "@chakra-ui/react": "1.6.10",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@meronex/icons": "4.0.0",
    "@next/bundle-analyzer": "11.1.2",
    "@sentry/browser": "6.13.3",
    "@sentry/node": "6.13.3",
    "apexcharts": "3.29.0",
    "axios": "0.23.0",
    "chalk": "^4.1.2",
    "cookies": "0.8.0",
    "cross-env": "7.0.3",
    "deepmerge": "4.2.2",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "framer-motion": "4.1.17",
    "graphql": "15.6.1",
    "howler": "2.2.3",
    "js-cookie": "3.0.1",
    "lodash.isequal": "4.5.0",
    "lodash.noop": "3.0.1",
    "next": "11.1.2",
    "next-apollo": "5.0.7",
    "next-compose-plugins": "2.2.1",
    "next-cookies": "2.0.3",
    "next-i18next": "8.9.0",
    "next-seo": "4.28.1",
    "react": "17.0.2",
    "react-apexcharts": "1.3.9",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "ts-node": "^10.3.0",
    "uuid": "8.3.2",
    "yup": "0.32.11",
    "zustand": "^3.5.14"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/chalk": "^2.2.0",
    "@types/cookies": "^0.7.7",
    "@types/dotenv": "^8.2.0",
    "@types/howler": "^2.2.4",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.0.0",
    "@types/lodash.noop": "^3.0.6",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@zeit/next-source-maps": "^0.0.3",
    "babel-eslint": "10.x",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-watch": "7.0.0",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-expect-message": "1.0.2",
    "jest-extended": "^1.1.0",
    "ngrok": "^4.2.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
