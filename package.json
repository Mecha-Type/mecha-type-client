{
  "name": "mecha-client",
  "version": "0.3.5",
  "description": "Client application for Mecha-Type",
  "license": "ISC",
  "private": true,
  "author": {
    "name": "Faustino Zanetto",
    "email": "zanettofaustino@gmail.com"
  },
  "scripts": {
    "start:dev": "next",
    "start:prod": "next start",
    "build": "yarn next build",
    "build:fast": "next build",
    "postbuild": "next-sitemap",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:once": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:once:group:no-integration": "cross-env NODE_ENV=test jest --group=-integration",
    "eslint": "next lint --fix --dir src",
    "prettier": "prettier -l \"src/**/*.{js,jsx,md,ts,tsx}\"",
    "lint": "yarn prettier && yarn eslint",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.4.0",
    "@tanstack/react-query": "^4.9.0",
    "@trpc/client": "^10.0.0-proxy-beta.13",
    "@trpc/next": "^10.0.0-proxy-beta.13",
    "@trpc/react": "^10.0.0-proxy-beta.13",
    "@trpc/server": "^10.0.0-proxy-beta.13",
    "clsx": "^1.2.1",
    "cssnano": "^5.1.13",
    "next": "^12.3.1",
    "next-auth": "^4.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.10.0",
    "ws": "^8.9.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.3.8",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/testing-library__react": "^10.2.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.7.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
